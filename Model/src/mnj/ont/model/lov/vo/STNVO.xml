<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="STNVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.vo.STNVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[Select ooha.HEADER_ID,
       ooha.ORDER_NUMBER,
       MSI.DESCRIPTION,
       msi.INVENTORY_ITEM_ID,
       ooha.ORDER_NUMBER||'/'||oola.line_number STN  ,
       oola.LINE_ID         
  FROM oe_order_headers_all         ooha,
       oe_order_lines_all           oola,
       mtl_system_items             msi,
       org_organization_definitions ood     
 Where oola.HEADER_ID = ooha.HEADER_ID
   and oola.INVENTORY_ITEM_ID = MSI.INVENTORY_ITEM_ID
   and ooha.org_id = ood.operating_unit
   and msi.organization_id = ood.organization_id
   and msi.inventory_item_id = oola.INVENTORY_ITEM_ID]]>
  </SQLQuery>
  <ViewAttribute
    Name="HeaderId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="HEADER_ID"
    Expression="HEADER_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="OrderNumber"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="ORDER_NUMBER"
    Expression="ORDER_NUMBER"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Description"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="240"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="DESCRIPTION"
    Expression="DESCRIPTION"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="240"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="InventoryItemId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="INVENTORY_ITEM_ID"
    Expression="INVENTORY_ITEM_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Stn"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="81"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="STN"
    Expression="STN"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="81"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="LineId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="LINE_ID"
    Expression="LINE_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnj.ont.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
