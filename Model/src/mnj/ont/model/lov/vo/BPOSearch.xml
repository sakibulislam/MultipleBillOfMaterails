<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BPOSearch"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.vo.BPOSearch_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_bom_id"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.sessionScope.bomIdS]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[select distinct bpo.bpo_no, bpo.sale_order_id
  from CUST_MNJ_ONT_SO_OBINSLINE    d,
       CUST_MNJ_ONT_SO_OBIN_SIZLINE l,
       MNJ_BOM_BPO_LINES_ALL        bpo
 where l.so_obinsline_id = d.so_obinsline_id
   and l.sales_order_id = bpo.sale_order_id
   and bpo.bom_id = :p_bom_id]]>
  </SQLQuery>
  <ViewAttribute
    Name="BpoNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BPO_NO"
    Expression="BPO_NO"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="500"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="20"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="SaleOrderId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="SALE_ORDER_ID"
    Expression="SALE_ORDER_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="BPOFlag"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VIEW_ATTR"
    SQLType="VARCHAR"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnj.ont.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
