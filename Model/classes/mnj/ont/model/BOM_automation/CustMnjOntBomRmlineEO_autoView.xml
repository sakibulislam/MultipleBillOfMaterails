         CustMnjOntBomRmlineLines.ATTRIBUTE5,
         CustMnjOntBomRmlineLines.ATTRIBUTE6,
         CustMnjOntBomRmlineLines.ATTRIBUTE7,
         CustMnjOntBomRmlineLines.ATTRIBUTE8,
         CustMnjOntBomRmlineLines.ATTRIBUTE9,
         CustMnjOntBomRmlineLines.ATTRIBUTE10,
         CustMnjOntBomRmlineLines.ATTRIBUTE11,
         CustMnjOntBomRmlineLines.ATTRIBUTE12,
         CustMnjOntBomRmlineLines.ATTRIBUTE13,
         CustMnjOntBomRmlineLines.ATTRIBUTE14,
         CustMnjOntBomRmlineLines.ATTRIBUTE15,
         CustMnjOntBomRmlineLines.VENDOR_ID,
         CustMnjOntBomRmlineLines.Flag,
         CustMnjOntBomRmlineLines.BOOKING_UOM,
         DECODE (msib.segment3,  '01', 'Local',  '02', 'Import',  'Leftover')
            Item_Status,
         msib.DESCRIPTION item_desc,
         (SELECT s.VENDOR_NAME
            FROM ap_suppliers s
           WHERE s.VENDOR_ID = CustMnjOntBomRmlineLines.vendor_id)
            supplier_name,
         msib.attribute1 legacy_code,
         CustMnjOntBomRmlineLines.Booked_Qty,
         CustMnjOntBomRmlineLines.Po_Qty,
         CustMnjOntBomRmlineLines.Delivery_Date1,
         CustMnjOntBomRmlineLines.Delivery_Date2,
         CustMnjOntBomRmlineLines.Delivery_Date3,
         CustMnjOntBomRmlineLines.Delivery_Date4,
         CustMnjOntBomRmlineLines.Delivery_Date5,
         CustMnjOntBomRmlineLines.Qty1,
         CustMnjOntBomRmlineLines.Qty2,
         CustMnjOntBomRmlineLines.Qty3,
         CustMnjOntBomRmlineLines.Qty4,
         CustMnjOntBomRmlineLines.Qty5,
         CustMnjOntBomRmlineLines.Uom_Conv_Rate,
         CustMnjOntBomRmlineLines.Onhand_Quantity,
         CustMnjOntBomRmlineLines.Allocate_Qty,
         CustMnjOntBomRmlineLines.Onhand_Qty1,
         CustMnjOntBomRmlineLines.Allocate_Qty1,
         CustMnjOntBomRmlineLines.Alocate_Intr_Qty,
           NVL (CustMnjOntBomRmlineLines.Booked_Qty, 0)
         - NVL (CustMnjOntBomRmlineLines.Alocate_Intr_Qty, 0)
            PR_QTY,
         CustMnjOntBomRmlineLines.Alocate_Cgl,
         CustMnjOntBomRmlineLines.Alocate_Gfl,
         CustMnjOntBomRmlineLines.Onhand_Cgl,
         CustMnjOntBomRmlineLines.Onhahnd_Gfl,
         CustMnjOntBomRmlineLines.Alocate_Intr_Cgl,
         CustMnjOntBomRmlineLines.Alocate_Inter_Gfl,
         CustMnjOntBomRmlineLines.Org_Id_Cal,
         CustMnjOntBomRmlineLines.Org_Id_Cgl,
         CustMnjOntBomRmlineLines.Org_Id_Gfl,
         DECODE (CustMnjOntBomRmlineLines.Approval_Flag,
                 'Y', 'Approved',
                 'R', 'Rejected',
                 'Not Approved')
            line_status,
         CustMnjOntBomRmlineLines.Exces_Buy_Reason,
         CustMnjOntBomRmlineLines.Price_Per_Unit,
         CustMnjOntBomRmlineLines.Total,
         CustMnjOntBomRmlineLines.Perntg,
         CustMnjOntBomRmlineLines.Proj_Pr_Qty,
         CUSTMNJONTBOMRMLINELINES.LEFT_OVER_QTY,
         CUSTMNJONTBOMRMLINELINES.ADDITIONAL_QTY,
         CUSTMNJONTBOMRMLINELINES.ACCUM_PRCNT,
         CUSTMNJONTBOMRMLINELINES.ACTUAL_PRCNT,
         CUSTMNJONTBOMRMLINELINES.PENDING_PRCNT,
         CUSTMNJONTBOMRMLINELINES.TRANSFER_QTY,
         CUSTMNJONTBOMRMLINELINES.CURRENCY,
         CUSTMNJONTBOMRMLINELINES.PURCHASE_TYPE,
         CUSTMNJONTBOMRMLINELINES.REMARKS
    FROM CUST_MNJ_ONT_BOM_RMLINE CustMnjOntBomRmlineLines,
         CUST_MNJ_ONT_BOM CustMnjOntBomHeader,
         mtl_system_items_b msib
   WHERE     msib.INVENTORY_ITEM_ID =
                CustMnjOntBomRmlineLines.Inventory_Item_Id
         AND CustMnjOntBomRmlineLines.BOM_ID = CUSTMNJONTBOMHEADER.BOM_ID
         AND msib.ORGANIZATION_ID = 353
         AND CustMnjOntBomRmlineLines.ATTRIBUTE1 IS NULL
ORDER BY DECODE (SUBSTR (CustMnjOntBomRmlineLines.ITEM_CODE, 1, 2),
                 11, 1,
                 12, 2,
                 13, 3,
                 21, 4,
                 22, 5,
                 24, 6,
                 23, 7,
                 28, 8,
                 25, 9,
                 26, 10,
                 30, 11,
                 32, 12,
                 27, 13,
                 29, 14,
                 36, 15,
                 31, 16,
                 34, 17,
                 55, 18) ASC]]>
  </SQLQuery>
