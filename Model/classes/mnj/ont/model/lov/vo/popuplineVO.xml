  <SQLQuery>
    <![CDATA[select distinct l.inventory_item_id,
       l.sales_order_id HEADER_ID,
       l.oc_no ob_ref,
       mnj_item_code(l.inventory_item_id, msib.organization_id) ORDERED_ITEM,
       msib.DESCRIPTION,
       TRIM(BOTH ' ' FROM l.dcpo_no) dcpo_no,
       l.inseam_qty,
       l.delivery_date,
       l.delivery_term,
       l.ship_mode,
       l.country,
       d.inseam_size_concat,
       d.size_qty,
       v.DESCRIPTION color,
       mbbla.bom_id

  from CUST_MNJ_ONT_SO_OBINSLINE    d,
       CUST_MNJ_ONT_SO_OBIN_SIZLINE l,
       mtl_system_items_b           msib,
       fnd_flex_values_vl           v,
       MNJ_BOM_BPO_LINES_ALL        mbbla
 where l.so_obinsline_id = d.so_obinsline_id
   and v.FLEX_VALUE_SET_ID = 1014895 --'MJ_Item_Shade/Color
   and v.flex_value = msib.segment4
   and v.enabled_flag = 'Y'
   and msib.ORGANIZATION_ID = 353
   and MBBLA.SALE_ORDER_ID = L.SALES_ORDER_ID
   and msib.inventory_item_id = l.inventory_item_id]]>
  </SQLQuery>
